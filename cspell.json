{
  "version": "0.2",
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "dictionaries": ["lorem", "vgv_allowed", "vgv_forbidden"],
  "dictionaryDefinitions": [
    {
      "name": "lorem",
      "path": "https://raw.githubusercontent.com/streetsidesoftware/cspell-dicts/refs/heads/main/dictionaries/lorem-ipsum/dict/lorem.txt",
      "description": "Lorem-ipsum dictionary for cspell."
    },
    {
      "name": "vgv_allowed",
      "path": "https://raw.githubusercontent.com/verygoodopensource/very_good_dictionaries/main/allowed.txt",
      "description": "Allowed VGV Spellings"
    },
    {
      "name": "vgv_forbidden",
      "path": "https://raw.githubusercontent.com/verygoodopensource/very_good_dictionaries/main/forbidden.txt",
      "description": "Forbidden VGV Spellings"
    }
  ],
  "ignorePaths": [
    ".vscode/*.json",
    "**/.gitignore",
    "**/*.entitlements",
    "**/*.pbxproj",
    "**/*.plist",
    "**/*.properties",
    "**/*.storyboard",
    "**/*.swift",
    "**/*.xcconfig",
    "**/*.xcscheme",
    "**/*.xib",
    "**/CMakeLists.txt",
    "**/google-services.json",
    "**/Podfile",
    "android/**/res/**/*.xml",
    "assets/svg/*.svg",
    "lib/firebase_options*.dart",
    "linux/my_application.cc",
    "shaders/*.frag",
    "windows/runner"
  ],
  "ignoreWords": ["flutterfx", "meta-data", "pbxproj"],
  "useGitignore": true,
  "allowCompoundWords": true,
  "suggestionsTimeout": 5000,
  "words": [
    "cupertino",
    "futureor",
    "interps",
    "ints",
    "lacunarity",
    "LTWH",
    "menlo",
    "pannable",
    "roboto",
    "vercel",
    "voronoi",
    "worley"
  ]
}
